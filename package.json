{
    "name": "md-outline",
    "displayName": "md-outline",
    "description": "outline for markdown file",
    "version": "0.0.1",
    "publisher": "yyfan",
    "engines": {
        "vscode": "^1.20.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:markdown",
        "onCommand:extension.sayHello"
    ],
    "main": "./out/extension",
    "contributes": {
        "views": {
            "explorer": [
                {
                    "id": "outline-view",
                    "name": "Markdown Outline"
                }
            ]
        },
        "commands": [
            {
                "command": "outline-view.add",
                "title": "Add"
            },
            {
                "command": "outline-view.delete",
                "title": "Delete"
            },
            {
                "command": "extension.sayHello",
                "title": "Hello World"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
